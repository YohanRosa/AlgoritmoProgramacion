//  Escribir un algoritmo que muestre un mensaje afirmativo si el número introducido es múltiplo de 5
Algoritmo CON03
	Definir numero como entero;
	
	Escribir "introduzca un numero entero: ";
	leer numero
	
	Si numero % 5 = 0 Entonces
		Escribir 'el numero introducido es multiplo de 5";
	SiNo
		Escribir 'el numero introducido no es multiplo de 5';
	FinSi
FinAlgoritmo

